int main(){
    // int x=102;
    // cout<<reverse(x);
    int n1,n2;
    
    char op;
    cout<<"enter num1"<<endl;
    cin>>n1;
    
    cout<<"enter operand"<<endl;
    cin>>op;

    cout<<"enter num2"<<endl;
    cin>>n2;
    

    switch(op){
        case '+':
        cout<<n1+n2;
        break;

        case '-':
        cout<<n1-n2;
        break;

        case '/':
        cout<<n1/n2;

        case '*':
        cout<<n1*n2;
        break ;

        default :
        cout<<"i am still leaing more!!";
        break;
    }


    return 0;
}
