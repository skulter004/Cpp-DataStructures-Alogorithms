#include<iostream>
using namespace std;



int main(){
   
    int amount;
    cout<<"enter the amount "<<endl;
    cin>>amount ;
   int rs100,rs50,rs20,rs1;
   
     switch (1){
         case 1: 
         rs100=amount/100;
         amount= amount%100;
         cout<<"100RS="<<rs100<<endl;
         
         

         case 2:
         rs50=amount/50;
         cout<<"50RS="<<rs50<<endl;
         amount = amount%50;
         

         case 3: 
         rs20=amount/20;
         cout<<"20RS="<<rs20<<endl;
         amount=amount%20;
         
         case 4:
         rs1=amount/1;
         cout<<"1RS="<<rs1<<endl;
         break;
         
     }
    

    return 0;
}
